{{!-- Added padding to the body to prevent the nav bar from overlapping with body content --}}
<link href="css/login_style.css" rel="stylesheet">

{{!-- Added sticky footer so footer sticks to the bottom of the page --}}
<link href="css/sticky_footer.css" rel="stylesheet">

<div class="container">

	<div class="row" id="usernameContainer">
		<div class="col-md-4 col-md-offset-4 welcomeText text-center">
			<h1>{{welcomeText}}</h1>
			<h3>{{message}}</h3>
		</div>
	</div>


<div class="col-md-4"></div>

	<div class="row">
		<div class="col-md-4">
		<form>
          
          <input id="adminname" name="adminname" placeholder="Admin" class="form-control input-lg" />
          
          <input type="password" name="password" class="form-control input-lg" id="password" placeholder="Password"/>

          <button id="submitAdmin" type="submit" class="btn btn-lg btn-primary btn-block">Submit</button>
          
        </form>
        </div>

</div>
<script type="text/javascript" src="js/adminlogin.js"></script>
