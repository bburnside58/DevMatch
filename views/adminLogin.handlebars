{{!-- Added padding to the body to prevent the nav bar from overlapping with body content --}}
<link href="css/login_style.css" rel="stylesheet">

<div class="row" id="usernameContainer">
	<div class="col-md-4 col-md-offset-4 welcomeText text-center">
		<h1>{{welcomeText}}</h1>
		<h3>{{message}}</h3>
	</div>
</div>
<br>

	<div class="row">
		<div class="col-md-4 col-md-offset-4 authInputs">
			<input id="adminname" name="adminname" placeholder="Admin" />
		</div>
	</div>
	<br>
	<div class="row">
		<div class="col-md-4 col-md-offset-4 authInputs">
			<input id="password" type="password" name="password" placeholder="Password" />
		</div>
	</div>
	<br>
	<div class="row">
		<div class="col-md-4 col-md-offset-4">
			<button class="btn btn-primary" type="submit" id="submitAdmin">Submit</button>
		</div>
	</div>

<br>

<script type="text/javascript" src="js/adminlogin.js"></script>